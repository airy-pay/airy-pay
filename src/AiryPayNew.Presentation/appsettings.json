{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Language": "ru-RU",
  "Kestrel": {
    "AllowedIPs": [
      "127.0.0.1",
      "172.21.0.1",
      "::1"
    ],
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      }
    }
  },
  "Discord": {
    "UseStagingServer": false,
    "StagingServerId": 0,
    "RateLimiters": [
      {
        "Limit": 3,
        "Period": "1s"
      },
      {
        "Limit": 200,
        "Period": "10m"
      },
      {
        "Limit": 1200,
        "Period": "2h"
      }
    ]
  }
}